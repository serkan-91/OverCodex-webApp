@{
    ViewData["Title"] = "Home page";
}
<!DOCTYPE html>
<html>
<head>
    <title>OverCodex</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <style>
        body, h1, h2, h3, h4, h5, h6 {
            font-family: "Raleway", sans-serif
        }

        body, html {
            height: 100%;
            line-height: 1.8;
        }

        /* Full height image header */
        .bgimg-1 {
            background-position: center;
            background-size: cover;
            background-image: url("/images/mac.jpg");
            min-height: 100%;
        }

        .w3-bar .w3-button {
            padding: 16px;
        }

        figure {
            text-align: center;
            margin: 0;
            background-color: black; /* Figürün arka plan rengi beyaz */
            padding: 10px; /* İsteğe bağlı: Metni biraz kenarlardan ayırır */
            height: 500px; /* Yükseklik 100 piksel */
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); /* İsteğe bağlı: Hafif bir gölgelendirme ekler */
            display: flex;
            flex-direction: column;
            align-items: center;
            color: honeydew;
            justify-content: center;
        }

            figure:hover {
                opacity: 0.85;
                cursor: pointer;
            }

        .ocxservices {
            position: relative;
            overflow: hidden;
        }

        .overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(255, 255, 255, 0.8);
            display: flex;
            align-items: center;
            justify-content: center;
            opacity: 0;
            transition: opacity 0.3s;
        }

        .ocxservices:hover .overlay {
            opacity: 1;
        }

        .overlay-content {
            text-align: center;
        }
    </style>
</head>
