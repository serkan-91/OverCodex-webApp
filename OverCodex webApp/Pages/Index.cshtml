<script>
    $(document).ready(function () {
        // Sayfa her kaydırıldığında kontrol et ve counter'ı başlat
        function checkCounters() {
            $(".counter").each(function () {
                if (isElementInViewport(this)) {
                    startCounter($(this));
                }
            });
        }

        // Sayfa her kaydırıldığında kontrol et
        $(document).on('scroll', checkCounters);

        // Sayıları artırmak için bir fonksiyon oluşturun
        function startCounter(element) {
            var endValue = parseInt(element.data("end-value"));
            var counterSpan = element.find(".w3-xxlarge");
            var currentValue = 0;

            $({ counter: currentValue }).animate(
                {
                    counter: endValue
                },
                {
                    duration: 3000, // Animasyon süresi (milisaniye cinsinden)
                    easing: 'swing', // Animasyon türü
                    step: function () {
                        // Sayıları güncelle
                        counterSpan.text(Math.ceil(this.counter));
                    }
                }
            );
        }

        // Sayfanın görünür kısmını kontrol etmek için bir fonksiyon
        function isElementInViewport(el) {
            var rect = el.getBoundingClientRect();
            return (
                rect.top >= 0 &&
                rect.left >= 0 &&
                rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
                rect.right <= (window.innerWidth || document.documentElement.clientWidth)
            );
        }
    });
</script>